{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-switch-case-fall-through": "off",
        "no-shadow": "off",
        "no-bitwise": "off",
        "no-param-reassign": 0,
        "no-constant-condition": 0,
        "no-restricted-syntax": 0
    }
}